<h3>会員一覧</h3>
<table class="table table-hover">
  <thead>
    <tr>
      <th>会員ID</th>
      <th>名前</th>
      <th>投稿数</th>
      <th>ステータス</th>
    </tr>
  </thead>
  <% @customers.each do |customer| %>
  <tbody>
    <tr>
      <td><%= customer.id %></td>
      <td><%= customer.nickname %></td>
      <td><%= customer.posts.count %></td>
      <td><%= customer.is_deleted %></td>
    </tr>
  </tbody>
  <% end %>
</table>