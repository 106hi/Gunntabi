<% if @post.get_image %>
  <% @post.image.each do |image| %>
    <%= image_tag image, size: "200x200" %>
  <% end %>
<% end %>
<h5>住所</h5>
<%= @post.address %><br>
投稿日：<%= @post.created_at.strftime('%Y/%m/%d') %>
<table>
  <thead>
    <tr>
      <th><%= @post.post_name %></th>
      <th>いいね</th>
    </tr>
  </thead>
  <tbody class="table">
    <tr>
      <th>おすすめポイント</th>
      <th colspan="3"></th>
    </tr>
    <td><%= @post.introduction %></td>
  </tbody>
</table>